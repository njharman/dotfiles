# <return>~? gives you the help screen that lists all the SSH escape sequences
# <return>~. escape a frozen SSH connection


Host *
  Protocol=2
  ForwardAgent=yes
  HashKnownHosts=yes
  AddressFamily=inet

# All for speed / multiple connections
  #Compression=yes
  #CompressionLevel=6 #1 fast, 9 slow(best)
  TCPKeepAlive=no
  ConnectTimeout=5
  ServerAliveInterval=60
  ServerAliveCountMax=60
  StrictHostKeyChecking=no
  GSSAPIAuthentication=no
  ## not in my ssh UseDNS=no
  #ControlMaster auto
  #ControlPath ~/.ssh/ssh_mux_%h_%p_%r
  # not in my ssh ControlPersist=4


Host normdev
  hostname 10.0.1.23
  user norman.harman
  # weak(fast) security for local
  Ciphers arcfour256
  MACS umac-64@openssh.com

Host github.com
  hostname github.com
  user njharman
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/github_rsa


